@model IEnumerable<Pro.Models.TaskModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="positioner">

    <div class="block Index_left">
        <h2>Текущие работы</h2>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Template)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Student)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Professor)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.isAccepted)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CountChanges)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.ActionLink(item.Name, "Task", new { id = item.Id })
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Template))
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Student)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Professor)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.isAccepted)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CountChanges)
                    </td>
                    <td>
                        @Html.ActionLink("Редактировать", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Удалить", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }
        </table>
    </div>

    <div class="block Index_right">
        <h2>Ваши шаблоны</h2>
        <ul>            
                        
        </ul>
</div>

</div>